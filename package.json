{
  "name": "svelte-window-variables",
  "version": "0.1.0",
  "description": "Svelte preprocessor to add component variables to the window during tests.",
  "module": "dist/main.es.js",
  "repository": "https://github.com/pngwn/svelte-window-variables",
  "scripts": {
    "test": "tape -r esm index.test.js",
    "autobuild": "rollup -c test/rollup.config.js -w",
    "dev": "run-p start:dev autobuild",
    "start:dev": "sirv test/public --dev"
  },
  "keywords": [
    "test",
    "preprocessor",
    "mdx",
    "markdown",
    "svelte"
  ],
  "author": "pngwn <hello@pngwn.io>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.11",
    "@types/node": "^10.12.12",
    "eslint": "^5.9.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.14.0",
    "esm": "^3.2.25",
    "highlight.js": "^9.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "prettier-eslint": "^8.8.2",
    "rollup": "^1.0.1",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.1.0",
    "rollup-plugin-svelte": "^5.1.0",
    "sirv-cli": "^0.4.4",
    "svelte": "^3.6.1",
    "tape": "^4.10.2"
  },
  "peerDependencies": {
    "svelte": "3.x"
  },
  "dependencies": {
    "meriyah": "^1.1.0"
  }
}
